<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mrl.mybatisplus.mapper.InfoMapper">

    <select id="selectByAge" resultType="com.mrl.mybatisplus.vo.InfoVo">
        select * from info where age &lt;= #{age}
    </select>
</mapper>
